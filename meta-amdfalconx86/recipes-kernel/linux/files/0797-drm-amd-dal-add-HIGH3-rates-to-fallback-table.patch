 /* 1080 Mbytes/sec*/
 { LANE_COUNT_TWO, LINK_RATE_HIGH2, LINK_SPREAD_DISABLED },
 /* 540 Mbytes/sec*/
 { LANE_COUNT_TWO, LINK_RATE_HIGH, LINK_SPREAD_DISABLED },
 /* 324 Mbytes/sec*/
 { LANE_COUNT_TWO, LINK_RATE_LOW, LINK_SPREAD_DISABLED },
+/* 1080 Mbytes/sec*/
