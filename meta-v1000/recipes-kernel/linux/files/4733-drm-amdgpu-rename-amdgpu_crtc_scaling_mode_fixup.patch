index 47dbdbe..6645388 100644
--- a/drivers/gpu/drm/amd/amdgpu/dce_v8_0.c
+++ b/drivers/gpu/drm/amd/amdgpu/dce_v8_0.c
@@ -2592,7 +2592,7 @@ static bool dce_v8_0_crtc_mode_fixup(struct drm_crtc *crtc,
